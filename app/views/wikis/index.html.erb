<div class="row">
  <div class="col-12">
    <div class="row">
      <div class="col-6">
        <h1>Wikis</h1>
      </div>
      <div class="col-6">
        <%= link_to "New Wiki", new_wiki_path, class: "btn btn-success float-right" %>
      </div>
    </div>
    <% @wiki.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <p><%= link_to wiki.title, wiki %></p>
          </h4>
        </div>
      </div>
      <% end %>
    </div>
</div>
